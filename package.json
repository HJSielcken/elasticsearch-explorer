{
  "name": "Elasticsearch-extension",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "CONFIG_ENV=dev PORT=8000 npm-run-all --parallel watch serve",
    "build": "NODE_ENV=production kaliber-esbuild",
    "pack-firefox": "web-ext build --source-dir ./target/",
    "build-firefox": "NODE_ENV=production CONFIG_ENV=dev BROWSER=firefox node ./build.js && yarn pack-firefox",
    "build-chrome": "NODE_ENV=production CONFIG_ENV=dev BROWSER=chrome node ./build.js",
    "watch": "kaliber-eswatch",
    "serve": "kaliber-esserve",
    "serve.dev": "PORT=8000 CONFIG_ENV=dev yarn serve"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7",
    "@kaliber/elasticsearch": "^2.1.2",
    "@kaliber/esbuild": "../kaliber-esbuild",
    "@kaliber/forms": "^2.1.2",
    "@kaliber/routing": "^2.0.0",
    "@kaliber/safe-json-stringify": "^2.0.1",
    "@kaliber/use-render-on-mount": "^1.0.4",
    "@tanstack/react-query": "^5.61.0",
    "classnames": "^2.5.1",
    "history": "^5.3.0",
    "hoist-non-react-statics": "^3.3.2",
    "import-fresh": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "web-ext": "^8.3.0"
  }
}
